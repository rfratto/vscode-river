{
    "name": "river",
    "displayName": "River",
    "description": "River configuration language support",
    "version": "0.1.0-alpha.3",
    "author": "Robert Fratto",
    "publisher": "rfratto",
    "repository": {
      "type": "git",
      "url": "https://github.com/rfratto/vscode-river"
    },
    "engines": {
        "vscode": "^1.69.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "river",
                "aliases": [
                    "River",
                    "river"
                ],
                "extensions": [
                    ".river",
                    ".rvr"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "river",
                "scopeName": "source.river",
                "path": "./syntaxes/river.tmLanguage.json"
            }
        ]
    }
}
